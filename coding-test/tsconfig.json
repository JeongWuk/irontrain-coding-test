{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES6",
    "module": "ES2015",
    "lib": ["dom"],
    "types": ["react", "react-dom", "node"],
    "jsx": "preserve",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  }
}
